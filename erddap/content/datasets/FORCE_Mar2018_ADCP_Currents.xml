      <dataType>float</dataType>
      
      <addAttributes>
        <att name="colorBarMaximum" type="double">0.5</att>
        <att name="colorBarMinimum" type="double">-0.5</att>
        <att name="ioos_category">Currents</att>
        <att name="long_name">Northward Sea Water Velocity</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>upward_sea_water_velocity</sourceName>
      <destinationName>upward_sea_water_velocity</destinationName>
      <dataType>float</dataType>
      
      <addAttributes>
        <att name="colorBarMaximum" type="double">0.5</att>
        <att name="colorBarMinimum" type="double">-0.5</att>
        <att name="ioos_category">Currents</att>
        <att name="long_name">Upward Sea Water Velocity</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>speed_of_sound_in_sea_water</sourceName>
      <destinationName>speed_of_sound_in_sea_water</destinationName>
      <dataType>float</dataType>
      
      <addAttributes>
        <att name="ioos_category">Currents</att>
        <att name="long_name">Speed Of Sound In Sea Water</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>cell_size</sourceName>
