      <att name="keywords">air, Air_Temp, area, atlantic, bottom, Bottom_T, cil, CIL_area, climate, data, department, dfo, dfo-mpo, distribution, fisheries, ice, ice distribution, icebergs, index, labrador, mpo, mun, nao, natural, newfoundland, north, ocean, oceans, oscillation, s27, S27_CIL, S27_S, S27_T, sea, Sea_Ice, signs, sst, surface, temperature, time, wint, Wint_NAO, year</att>
